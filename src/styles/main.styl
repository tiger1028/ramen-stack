@import 'yeticss'

.color.red
  color: $red

.color.green
  color: $green

.color.orange
  color: $orange

.hidden
  display: none
  visibility: hidden

header
  padding-top: 50px

.label
  height: 40px

  .label-color
    width: 24px
    height: 24px
    border: 1px solid grey
    border-radius: 20px
    display: inline-block

  > span
    margin-left: 10px
    margin-right: 10px
